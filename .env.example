# .env.example
# Orbiton.ai API 환경변수 예시 파일

# 서버 설정
HOST=0.0.0.0
PORT=8000
WORKERS=1
ENV=development

# Slack 설정
SLACK_SIGNING_SECRET=your-slack-signing-secret
SLACK_VERIFICATION_TOKEN=your-slack-verification-token

# ClickUp 설정
CLICKUP_API_KEY=your-clickup-api-key
CLICKUP_LIST_ID=your-clickup-list-id

# PostgreSQL DB URL
DATABASE_URL=postgresql://username:password@localhost:5432/orbiton_db

# GeoIP 설정
GEOIP_DB_PATH=GeoLite2-Country.mmdb
ALLOWED_COUNTRIES=KR,US,JP